{
  "name": "postcss-remove-redundancy",
  "version": "0.0.0",
  "description": "PostCSS plugin that removes redundant rules for a slimmer stylesheet",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "redundant",
    "redundancy",
    "deduplicate",
    "dedupe"
  ],
  "author": "tjgavlick <8879532+tjgavlick@users.noreply.github.com>",
  "license": "MIT",
  "repository": "tjgavlick/postcss-remove-redundancy",
  "bugs": {
    "url": "https://github.com/tjgavlick/postcss-remove-redundancy/issues"
  },
  "homepage": "https://github.com/tjgavlick/postcss-remove-redundancy",
  "scripts": {
    "test": "jest && eslint *.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "jest": true,
      "es6": true,
      "node": true
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "semi": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "warn",
        "only-multiline"
      ]
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "postcss": "^7.0.26",
    "postcss-selector-parser": "^6.0.2"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "jest": "^24.9.0"
  }
}
